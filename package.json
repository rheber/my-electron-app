{
  "name": "my-electron-app",
  "productName": "Ahoy",
  "version": "1.0.0",
  "main": "app/public/index.js",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "run-s build:react build:package",
    "build:package": "electron-packager . --asar --overwrite --out=release/",
    "build:react": "webpack --mode development",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "dev:serve": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "start": "run-p -r dev:*"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "asar": "^0.14.3",
    "babel-loader": "^8.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.8",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.1.1",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  }
}
